{
  "name": "Server: Core",
  "service": "arsnova-server-core",
  "dockerComposeFile": [
    "${localEnv:PDK_COMPOSE_FILE}"
  ],
  "workspaceFolder": "/workspace/core",
  "customizations": {
    "vscode": {
      "extensions": [
        "asciidoctor.asciidoctor-vscode",
        "eamodio.gitlens",
        "editorconfig.editorconfig",
        "pivotal.vscode-spring-boot",
        "vscjava.vscode-java-pack"
      ]
    }
  },
  "shutdownAction": "none",
  "remoteUser": "dev",
  "postAttachCommand": "/post-attach.sh",
  "features": {
    "ghcr.io/devcontainers/features/dotnet:2": {
        "version": "8.0"
    }
  }
}
