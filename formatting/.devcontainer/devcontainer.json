{
  "name": "Server: Formatting",
  "service": "arsnova-server-formatting",
  "dockerComposeFile": [
    "${localEnv:PDK_COMPOSE_FILE}"
  ],
  "workspaceFolder": "/workspace/formatting",
  "customizations": {
    "vscode": {
      "extensions": [
        "asciidoctor.asciidoctor-vscode",
        "dbaeumer.vscode-eslint",
        "eamodio.gitlens",
        "editorconfig.editorconfig"
      ]
    }
  },
  "shutdownAction": "none",
  "remoteUser": "dev",
  "postAttachCommand": "/post-attach.sh"
}
